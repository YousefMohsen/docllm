{
  "name": "docllm",
  "version": "0.1.0",
  "private": true,
  "description": "Document + entity pipeline; MCP for media-investigation queries.",
  "scripts": {
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "process_documents": "tsx src/process_documents.ts",
    "chunk_documents": "tsx src/chunk_documents.ts",
    "create_chunk_embeddings": "tsx src/create_chunk_embeddings.ts",
    "run_pipeline": "npm run process_documents && npm run chunk_documents && npm run create_chunk_embeddings",
    "print_first_embeddings": "tsx scripts/print_first_embeddings.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "commander": "^14.0.3",
    "dotenv": "^17.3.1",
    "pdf-parse": "^2.4.5"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@types/pdf-parse": "^1.1.5",
    "prisma": "^6.0.0",
    "tsx": "^4.20.0",
    "typescript": "^5.8.0"
  }
}
